                                                  "NodeIsActive": true,
                                                  "NodePortraitBitmap": "",
                                                  "NodeNpcName": "",
                                                  "NodeSound": "none",
                                                  "conversationText": "\"Middle One! The child!\"",
                                                  "IsExpanded": true,
                                                  "subNodes": [
                                                    {
