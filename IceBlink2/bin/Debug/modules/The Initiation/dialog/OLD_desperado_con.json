                          "ShowOnlyOnce": false,
                          "NodeIsActive": true,
                          "NodePortraitBitmap": "",
                          "NodeNpcName": "",
                          "NodeSound": "none",
                          "conversationText": "\"Yes -- he must have been there, too.\"",
                          "IsExpanded": true,
                          "subNodes": [
                            {
                              "idNum": 6,
                              "orderNum": 0,
