                                          "pcNode": false,
                                          "linkTo": 0,
                                          "ShowOnlyOnce": false,
                                          "NodeIsActive": true,
                                          "NodePortraitBitmap": "",
                                          "NodeNpcName": "",
                                          "NodeSound": "none",
                                          "conversationText": "\"There's no time.\"",
                                          "IsExpanded": true,
                                          "subNodes": [
                                            {
                                              "idNum": 10,
                                              "orderNum": 0,
                                              "pcNode": true,
                                              "linkTo": 0,
