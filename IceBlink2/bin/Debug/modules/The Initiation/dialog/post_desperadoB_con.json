                  "linkTo": 0,
                  "ShowOnlyOnce": false,
                  "NodeIsActive": true,
                  "NodePortraitBitmap": "prp_witchparty3.png",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "\"Are you all right? There -- lean on me.\"\r\n\r\nHe put his arm around her, offering his body as a support.",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 162,
                      "orderNum": 0,
                      "pcNode": true,
                      "linkTo": 0,
                      "ShowOnlyOnce": false,
                      "NodeIsActive": true,
                      "NodePortraitBitmap": "",
                      "NodeNpcName": "",
                      "NodeSound": "none",
                      "conversationText": "[Continue.]",
                      "IsExpanded": true,
                      "subNodes": [
                        {
                          "idNum": 163,
                          "orderNum": 0,
                          "pcNode": false,
                          "linkTo": 0,
                          "ShowOnlyOnce": false,
                          "NodeIsActive": true,
                          "NodePortraitBitmap": "prp_hedgemagician.png",
                          "NodeNpcName": "",
                          "NodeSound": "none",
                          "conversationText": "But the magician wriggled away.\r\n\r\n\"Zaurus can't use you as a shield forever,\" she said to <FirstName>.",
                          "IsExpanded": true,
                          "subNodes": [
                            {
                              "idNum": 165,
                              "orderNum": 0,
                              "pcNode": true,
                              "linkTo": 0,
                              "ShowOnlyOnce": false,
                              "NodeIsActive": true,
                              "NodePortraitBitmap": "",
                              "NodeNpcName": "",
                              "NodeSound": "none",
                              "conversationText": "[Continue.]",
                              "IsExpanded": true,
                              "subNodes": [
                                {
                                  "idNum": 166,
