                                                  "pcNode": false,
                                                  "linkTo": 0,
                                                  "ShowOnlyOnce": false,
                                                  "NodeIsActive": true,
                                                  "NodePortraitBitmap": "prp_witchparty3.png",
                                                  "NodeNpcName": "",
                                                  "NodeSound": "none",
                                                  "conversationText": "\"Speak for yourself.\"",
                                                  "IsExpanded": true,
                                                  "subNodes": [
                                                    {
                                                      "idNum": 177,
                                                      "orderNum": 0,
                                                      "pcNode": true,
                                                      "linkTo": 0,
