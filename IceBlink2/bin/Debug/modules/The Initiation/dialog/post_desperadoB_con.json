                                                      "NodeIsActive": true,
                                                      "NodePortraitBitmap": "",
                                                      "NodeNpcName": "",
                                                      "NodeSound": "none",
                                                      "conversationText": "[Continue.]",
                                                      "IsExpanded": true,
                                                      "subNodes": [
