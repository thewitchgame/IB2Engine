                      "orderNum": 0,
                      "pcNode": true,
                      "linkTo": 0,
                      "ShowOnlyOnce": false,
                      "NodeIsActive": true,
                      "NodePortraitBitmap": "",
                      "NodeNpcName": "",
                      "NodeSound": "none",
                      "conversationText": "\"What is this place?\"",
                      "IsExpanded": true,
